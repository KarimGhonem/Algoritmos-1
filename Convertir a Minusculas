#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include <stdio.h>
#include <string.h>
using namespace std;


void main()
{
   char Cadena[100];
   cout<<"Ingrese su cadena: "<<endl;
   gets(Cadena);
   cout<<"string prior to strlwr: "<<Cadena<<endl;
   strlwr(Cadena);
   cout<<"string after strlwr:"<<Cadena<<endl;
   getch();
}
